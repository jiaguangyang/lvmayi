<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jk.dao.LvMapper">
    <insert id="addBiaoti">
        insert into t_biaoti(text,uri) values(#{text},#{uri})
    </insert>
    <update id="updateBiaoti">
        update t_biaoti <set>
        <if test="text!='' and text!=null">
            text=#{text},
        </if>
        <if test="uri!='' and uri!=null">
            uri=#{uri}
        </if>

    </set>
        where id=#{id}
    </update>
   <!-- 查询 承运商 分页 条查-->
    <select id="getcommon" resultType="com.jk.model.common">

    </select>

</mapper>